{
  "name": "solid-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve ./_docs ./docs --open",
    "docs:build": "npm run docs:prepare && gitbook build ./_docs ./docs",
    "extract-i18n": "ng xi18n solid-app --i18n-format xlf --output-path locale --i18n-locale en && ng run solid-app:xliffmerge",
    "start-en": "ng serve --configuration=en",
    "cucumber": "cucumber-js",
    "ghpages-build": "ng build --prod --base-href \"https://arquisoft.github.io/dechat_en1b/\"",
    "ghpages-launch": "ngh --dir dist/solid-app"
  },
  "browser": {
    "crypto": false,
    "fetch": false,
    "window": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.1.4",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@ng-select/ng-select": "^2.5.1",
    "@ngx-i18nsupport/tooling": "^1.1.2",
    "@types/chart.js": "^2.7.48",
    "angular-cli-ghpages": "^0.5.3",
    "chart.js": "^2.5.0",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-theme-api": "^1.1.2",
    "hammerjs": "^2.0.8",
    "http": "^0.0.0",
    "internationalization": "^0.1.2",
    "isomorphic-fetch": "^2.2.1",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-toastr": "^9.0.2",
    "node-fetch": "^2.2.0",
    "npm": "^6.9.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "purecss": "^1.0.0",
    "rdflib": "^0.19.0",
    "rxjs": "^6.0.0",
    "solid-auth-client": "^2.3.0",
    "solid-file-client": "^0.4.9",
    "stdio": "^0.2.7",
    "text-encoding": "^0.6.4",
    "webcrypto": "^0.1.1",
    "whatwg-url": "^7.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.5",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^6.0.3",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "~4.2.1",
    "coveralls": "^3.0.3",
    "cucumber": "^5.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "^3.2.4"
  }
}
